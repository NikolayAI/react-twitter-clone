 {\n  const history = useHistory();\n  const dispatch = useDispatch();\n\n  const checkAuth = async () => {\n    try {\n      const { data } = await authApi.me();\n      dispatch(setUserData(data));\n      history.replace('/home');\n    } catch (error) {\n      console.log(error);\n    }\n  };\n\n  useEffect(() => {\n    checkAuth();\n  }, []);\n\n  return (\n    <div className=\"App\">\n      <Switch>\n        <Route path={'/signin'} render={() => <SignIn/>}/>\n        <Route path={'/'} render={() => <Home/>}/>\n      </Switch>\n    </div>\n  );\n}\n\nexport default App;\n","/home/nikolay/development/reactProjects/react-twitter-clone/src/components/AddTweetForm/AddTweetForm.tsx",[],"/home/nikolay/development/reactProjects/react-twitter-clone/src/components/BackButton/BackButton.tsx",[],"/home/nikolay/development/reactProjects/react-twitter-clone/src/components/ModalBlock/ModalBlock.tsx",[],"/home/nikolay/development/reactProjects/react-twitter-clone/src/components/Notification/Notification.tsx",[],"/home/nikolay/development/reactProjects/react-twitter-clone/src/components/SearchTextField/SearchTextField.tsx",[],"/home/nikolay/development/reactProjects/react-twitter-clone/src/components/SideMenu/SideMenu.tsx",[],"/home/nikolay/development/reactProjects/react-twitter-clone/src/components/Tags/Tags.tsx",[],"/home/nikolay/development/reactProjects/react-twitter-clone/src/components/Tweet/Tweet.tsx",[],"/home/nikolay/development/reactProjects/react-twitter-clone/src/core/axios.ts",[],"/home/nikolay/development/reactProjects/react-twitter-clone/src/index.tsx",[],"/home/nikolay/development/reactProjects/react-twitter-clone/src/pages/Home/Home.tsx",["252","253","254","255","256","257","258","259"],"import React from 'react';\nimport { Grid } from '@material-ui/core';\nimport Paper from '@material-ui/core/Paper';\nimport Typography from '@material-ui/core/Typography';\nimport Container from '@material-ui/core/Container';\nimport { Tweet } from '../../components/Tweet/Tweet';\nimport { SideMenu } from '../../components/SideMenu/SideMenu';\nimport Avatar from '@material-ui/core/Avatar';\nimport Button from '@material-ui/core/Button';\nimport InputAdornment from '@material-ui/core/InputAdornment';\nimport SearchIcon from '@material-ui/icons/SearchOutlined';\nimport List from '@material-ui/core/List';\nimport ListItem from '@material-ui/core/ListItem';\nimport ListItemText from '@material-ui/core/ListItemText';\nimport Divider from '@material-ui/core/Divider';\nimport ListItemAvatar from '@material-ui/core/ListItemAvatar';\nimport PersonAddIcon from '@material-ui/icons/PersonAddOutlined';\nimport { AddTweetForm } from '../../components/AddTweetForm/AddTweetForm';\nimport { useHomeStyles } from './homeTheme';\nimport { SearchTextField } from '../../components/SearchTextField/SearchTextField';\nimport { useDispatch, useSelector } from 'react-redux';\nimport {\n  selectTweetsIsLoading,\n  selectTweetsItems\n} from '../../store/ducks/tweets/selectors';\nimport CircularProgress from '@material-ui/core/CircularProgress';\nimport { Tags } from '../../components/Tags/Tags';\nimport { fetchTweets } from '../../store/ducks/tweets/actionCreators';\nimport { fetchTags } from '../../store/ducks/tags/actionCreators';\nimport { Route } from 'react-router-dom';\nimport { BackButton } from '../../components/BackButton/BackButton';\nimport { FullTweet } from './components/FullTweet';\nimport { Users } from '../../components/Users/Users';\n\nexport const Home: React.FC = () => {\n\n  const dispatch = useDispatch();\n  const classes = useHomeStyles();\n  const tweets = useSelector(selectTweetsItems);\n  const isLoading = useSelector(selectTweetsIsLoading);\n\n  React.useEffect(() => {\n    dispatch(fetchTweets());\n    dispatch(fetchTags());\n  }, [dispatch]);\n\n  return (\n    <Container className={classes.wrapper} maxWidth={'lg'}>\n      <Grid container spacing={3}>\n        <Grid sm={1} md={3} item>\n          <SideMenu classes={classes}/>\n        </Grid>\n        <Grid sm={8} md={6} item>\n          <Paper className={classes.tweetsWrapper} variant={'outlined'}>\n            <Paper className={classes.tweetsHeader} variant={'outlined'}>\n              <Route path={'/home/:any'}>\n                <BackButton/>\n              </Route>\n              <Route path={['/home', '/home/search']} exact>\n                <Typography variant={'h6'}>Твиты</Typography>\n              </Route>\n              <Route path={'/home/tweet'}>\n                <Typography variant={'h6'}>Твитнуть</Typography>\n              </Route>\n            </Paper>\n            <Route path={['/home', '/home/search']} exact>\n              <Paper>\n                <div className={classes.addForm}>\n                  <AddTweetForm classes={classes}/>\n                </div>\n                <div className={classes.addFormBottomLine}/>\n              </Paper>\n            </Route>\n            <Route path={'/home'} exact>\n              {isLoading ? (\n                <div className={classes.tweetsCentered}><CircularProgress/>\n                </div>\n              ) : tweets.map(tweet =>\n                <Tweet\n                  _id={tweet._id}\n                  key={tweet._id}\n                  classes={classes}\n                  text={tweet.text}\n                  user={tweet.user}\n                  createdAt={tweet.createdAt}\n                />\n              )}\n            </Route>\n            <Route path={'/home/tweet/:id'} exact component={FullTweet}/>\n          </Paper>\n        </Grid>\n        <Grid sm={3} md={3} item>\n          <div className={classes.rightSide}>\n            <SearchTextField\n              fullWidth\n              variant={'outlined'}\n              placeholder={'Поиск по Твиттеру'}\n              InputProps={{\n                startAdornment: (\n                  <InputAdornment position={'start'}>\n                    <SearchIcon/>\n                  </InputAdornment>\n                ),\n              }}\n   